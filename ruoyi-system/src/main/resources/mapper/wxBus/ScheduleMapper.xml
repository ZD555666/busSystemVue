<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.wx.wxBus.mapper.ScheduleMapper">

    <select id="selectSchedule" resultType="com.ruoyi.wx.wxBus.domain.MySchedule">
        select * from t_schedule where busNo = #{busNo} order by starTime;
    </select>


</mapper>
