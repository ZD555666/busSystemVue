<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.wx.wxuser.mapper.MsgTemplateMapper">

    <select id="queryAllTempByCode" resultType="MsgTemplate">
        select *
        from t_msgtemplate
        where templateCode = #{templateCode};
    </select>

</mapper>