<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.wx.wxuser.mapper.WxScheduleMapper">

    <select id="queryScheduled" resultType="WxSchedule">
        select *
        from t_schedule
        where busState = #{busState1}
           or busState = #{busState2}
        order By starTime;
    </select>
</mapper>
